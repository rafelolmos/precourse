function meuNom(myName){
  return(myName)
}
var myName = meuNom('Rafel')

myName

function salutacio(myName, myAge){
  return ('Hola '+ myName + ', tens '+ myAge + ' anys')
} 

var myFunction = salutacio('Rafel', 40)

myFunction

function anys(myAge) {
  return(myAge)
}
var myAge = anys(40)

myAge

myAge + myName
